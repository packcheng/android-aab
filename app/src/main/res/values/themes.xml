<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AndroidAab" parent="Theme.AppCompat.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="android:colorSecondary">@color/purple_500</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.AndroidAab.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.AndroidAab.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.AndroidAab.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>